{
  "name": "storybook-color-picker",
  "description": "Choose a color from custom color palette and set it on component's control and/or copy it to clipboard.",
  "version": "4.0.0",
  "author": "Adrian_Bielawski",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/ts/index.d.ts",
  "files": [
    "dist/**/*",
    "!dist/**/*.spec.js",
    "!dist/**/testsUtils.js",
    "preset.js"
  ],
  "license": "ISC",
  "scripts": {
    "clean": "rimraf ./dist",
    "buildBabel": "concurrently \"npm run buildBabel:cjs\" \"npm run buildBabel:esm\"",
    "buildBabel:files": "babel ./src --extensions \".js,.jsx,.ts,.tsx\"",
    "buildBabel:cjs": "npm run buildBabel:files -- -d ./dist/cjs",
    "buildBabel:esm": "npm run buildBabel:files -- -d ./dist/esm --env-name esm",
    "buildTsc": "tsc --declaration --emitDeclarationOnly --outDir ./dist/ts",
    "build-storybook": "storybook build",
    "prebuild": "npm run clean",
    "build": "concurrently \"npm run buildBabel\" \"npm run buildTsc\"",
    "build:watch": "concurrently \"npm run buildBabel:esm -- --watch\" \"npm run buildTsc -- --watch\"",
    "prepare": "husky install",
    "prettier:check": "prettier --check '{src,test}/**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write '{src,test}/**/*.{ts,tsx}'",
    "prettier:staged": "npm run prettier:check -- --staged",
    "lint": "eslint src --ext .ts,.tsx --cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev",
    "start": "concurrently \"npm run build:watch\" \"npm run storybook -- --quiet\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "chroma-js": "^2.4.2",
    "copy-to-clipboard": "^3.3.3",
    "pluralize": "^8.0.0",
    "react-animate-height": "^3.2.2",
    "react-popper-tooltip": "^4.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@emotion/eslint-plugin": "^11.10.0",
    "@storybook/addon-actions": "^7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/blocks": "^7.4.0",
    "@storybook/components": "^7.4.0",
    "@storybook/manager-api": "^7.4.0",
    "@storybook/mdx2-csf": "^1.1.0",
    "@storybook/preview-api": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/react-webpack5": "^7.4.0",
    "@storybook/theming": "^7.4.0",
    "@testing-library/react": "^14.0.0",
    "@types/chroma-js": "^2.1.4",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.7.4",
    "babel-jest": "^29.6.4",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-environment-jsdom-global": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.4.0",
    "storybook-dark-mode": "^3.0.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2"
  },
  "peerDependencies": {
    "@storybook/blocks": "^7.0.0",
    "@storybook/components": "^7.0.0",
    "@storybook/core-events": "^7.0.0",
    "@storybook/manager-api": "^7.0.0",
    "@storybook/preview-api": "^7.0.0",
    "@storybook/theming": "^7.0.0",
    "@storybook/types": "^7.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint",
      "npm run prettier:staged"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrianbielawski/storybook-color-picker.git"
  },
  "bugs": {
    "url": "https://github.com/adrianbielawski/storybook-color-picker/issues"
  },
  "homepage": "https://github.com/adrianbielawski/storybook-color-picker#readme",
  "keywords": [
    "storybook-addons",
    "essentials",
    "design",
    "style",
    "storybook",
    "addon",
    "color",
    "picker",
    "palette"
  ],
  "storybook": {
    "displayName": "Color picker",
    "unsupportedFrameworks": [
      "react-native"
    ],
    "icon": "https://user-images.githubusercontent.com/57548494/123546997-dbd10500-d756-11eb-9935-2c74087f295e.png"
  }
}
