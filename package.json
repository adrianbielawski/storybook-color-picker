{
  "name": "storybook-color-picker",
  "description": "Custom palette color picker addon for storybook",
  "version": "1.0.7",
  "author": "Adrian_Bielawski",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/ts/index.d.ts",
  "files": [
    "dist/**/*",
    "*.js",
    "*.d.ts"
  ],
  "license": "ISC",
  "scripts": {
    "clean": "rimraf ./dist",
    "buildBabel": "concurrently \"yarn buildBabel:cjs\" \"yarn buildBabel:esm\"",
    "buildBabel:cjs": "babel ./src -d ./dist/cjs --extensions \".js,.jsx,.ts,.tsx\"",
    "buildBabel:esm": "babel ./src -d ./dist/esm --env-name esm --extensions \".js,.jsx,.ts,.tsx\"",
    "buildTsc": "tsc --declaration --emitDeclarationOnly --outDir ./dist/ts",
    "prebuild": "yarn clean",
    "build": "concurrently \"yarn buildBabel\" \"yarn buildTsc\"",
    "build:watch": "concurrently \"yarn buildBabel:esm -- --watch\" \"yarn buildTsc -- --watch\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "start": "concurrently \"yarn build:watch\" \"yarn storybook -- --no-manager-cache --quiet\"",
    "build-storybook": "build-storybook",
    "publish": "concurrently \"yarn build\" \"npm publish\""
  },
  "dependencies": {
    "@babel/cli": "^7.14.5",
    "@emotion/react": "^11.4.0",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "concurrently": "^6.2.0",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-popper-tooltip": "^4.3.0",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/react": "^6.2.9",
    "babel-loader": "^8.2.2"
  },
  "peerDependencies": {
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "react": "^17.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrianbielawski/storybook-color-picker.git"
  },
  "keywords": [
    "storybook",
    "addon",
    "color",
    "picker"
  ],
  "bugs": {
    "url": "https://github.com/adrianbielawski/storybook-color-picker/issues"
  },
  "homepage": "https://github.com/adrianbielawski/storybook-color-picker#readme"
}
